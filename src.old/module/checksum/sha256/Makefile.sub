STORIQ_ARCHIVER_CHECKSUM_SHA256_SRC_DIR		:= src/module/checksum/sha256
STORIQ_ARCHIVER_CHECKSUM_SHA256_BUILD_DIR	:= ${BUILD_DIR}/module/checksum/sha256
STORIQ_ARCHIVER_CHECKSUM_SHA256_DEPEND_DIR	:= ${DEPEND_DIR}/module/checksum/sha256

STORIQ_ARCHIVER_CHECKSUM_SHA256_SRC_FILES	:= $(sort $(shell test -d ${STORIQ_ARCHIVER_CHECKSUM_SHA256_SRC_DIR} && find ${STORIQ_ARCHIVER_CHECKSUM_SHA256_SRC_DIR} -name '*.c'))
STORIQ_ARCHIVER_CHECKSUM_SHA256_HEAD_FILES	:= $(sort $(shell test -d ${STORIQ_ARCHIVER_CHECKSUM_SHA256_SRC_DIR} && find ${STORIQ_ARCHIVER_CHECKSUM_SHA256_SRC_DIR} -name '*.h'))
STORIQ_ARCHIVER_CHECKSUM_SHA256_OBJ_FILES	:= $(sort $(patsubst src/%.c,${BUILD_DIR}/%.o,${STORIQ_ARCHIVER_CHECKSUM_SHA256_SRC_FILES}))
STORIQ_ARCHIVER_CHECKSUM_SHA256_DEP_FILES	:= $(sort $(shell test -d ${STORIQ_ARCHIVER_CHECKSUM_SHA256_DEPEND_DIR} && find ${STORIQ_ARCHIVER_CHECKSUM_SHA256_DEPEND_DIR} -name '*.d'))

STORIQ_ARCHIVER_CHECKSUM_SHA256_BIN			:= lib/checksum/libsha256.so
STORIQ_ARCHIVER_CHECKSUM_SHA256_CFLAG		:= -fPIC -fvisibility=hidden
STORIQ_ARCHIVER_CHECKSUM_SHA256_LD			:= -shared -lssl

STORIQ_ARCHIVER_CHECKSUM_SHA256_LIB			:=

BIN_SYMS									+= STORIQ_ARCHIVER_CHECKSUM_SHA256
