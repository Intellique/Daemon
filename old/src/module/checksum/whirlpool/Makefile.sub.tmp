STONE_CHECKSUM_WHIRLPOOL_SRC_DIR		:= src/module/checksum/whirlpool
STONE_CHECKSUM_WHIRLPOOL_BUILD_DIR		:= ${BUILD_DIR}/module/checksum/whirlpool
STONE_CHECKSUM_WHIRLPOOL_DEPEND_DIR		:= ${DEPEND_DIR}/module/checksum/whirlpool

STONE_CHECKSUM_WHIRLPOOL_SRC_FILES		:= $(sort $(shell test -d ${STONE_CHECKSUM_WHIRLPOOL_SRC_DIR} && find ${STONE_CHECKSUM_WHIRLPOOL_SRC_DIR} -name '*.c'))
STONE_CHECKSUM_WHIRLPOOL_HEAD_FILES		:= $(sort $(shell test -d ${STONE_CHECKSUM_WHIRLPOOL_SRC_DIR} && find ${STONE_CHECKSUM_WHIRLPOOL_SRC_DIR} -name '*.h'))
STONE_CHECKSUM_WHIRLPOOL_OBJ_FILES		:= $(sort $(patsubst src/%.c,${BUILD_DIR}/%.o,${STONE_CHECKSUM_WHIRLPOOL_SRC_FILES}))
STONE_CHECKSUM_WHIRLPOOL_DEP_FILES		:= $(sort $(shell test -d ${STONE_CHECKSUM_WHIRLPOOL_DEPEND_DIR} && find ${STONE_CHECKSUM_WHIRLPOOL_DEPEND_DIR} -name '*.d'))

STONE_CHECKSUM_WHIRLPOOL_BIN			:= lib/libchecksum-whirlpool.so
STONE_CHECKSUM_WHIRLPOOL_CFLAG			:= -pthread -fPIC -fvisibility=hidden
STONE_CHECKSUM_WHIRLPOOL_LD				:= -pthread -shared -lcrypto

STONE_CHECKSUM_WHIRLPOOL_LIB			:=

BIN_SYMS								+= STONE_CHECKSUM_WHIRLPOOL
