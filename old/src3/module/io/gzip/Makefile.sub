STORIQ_ARCHIVER_IO_GZIP_SRC_DIR		:= src/module/io/gzip
STORIQ_ARCHIVER_IO_GZIP_BUILD_DIR	:= ${BUILD_DIR}/module/io/gzip
STORIQ_ARCHIVER_IO_GZIP_DEPEND_DIR	:= ${DEPEND_DIR}/module/io/gzip

STORIQ_ARCHIVER_IO_GZIP_SRC_FILES	:= $(sort $(shell test -d ${STORIQ_ARCHIVER_IO_GZIP_SRC_DIR} && find ${STORIQ_ARCHIVER_IO_GZIP_SRC_DIR} -name '*.c'))
STORIQ_ARCHIVER_IO_GZIP_HEAD_FILES	:= $(sort $(shell test -d ${STORIQ_ARCHIVER_IO_GZIP_SRC_DIR} && find ${STORIQ_ARCHIVER_IO_GZIP_SRC_DIR} -name '*.h'))
STORIQ_ARCHIVER_IO_GZIP_OBJ_FILES	:= $(sort $(patsubst src/%.c,${BUILD_DIR}/%.o,${STORIQ_ARCHIVER_IO_GZIP_SRC_FILES}))
STORIQ_ARCHIVER_IO_GZIP_DEP_FILES	:= $(sort $(shell test -d ${STORIQ_ARCHIVER_IO_GZIP_DEPEND_DIR} && find ${STORIQ_ARCHIVER_IO_GZIP_DEPEND_DIR} -name '*.d'))

STORIQ_ARCHIVER_IO_GZIP_BIN			:= lib/io/libgzip.so
STORIQ_ARCHIVER_IO_GZIP_CFLAG		:= -fPIC -fvisibility=hidden
STORIQ_ARCHIVER_IO_GZIP_LD			:= -shared -lz

STORIQ_ARCHIVER_IO_GZIP_LIB			:=

BIN_SYMS							+= STORIQ_ARCHIVER_IO_GZIP
