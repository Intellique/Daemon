#: src/logger/file.c:147
msgid   "Log: file: logrotate detected"
msgstr  "Log : file : logrotate détecté"

#: src/logger/listen.c:54
msgid   "New connection..."
msgstr  "Nouvelle connexion..."

#: src/logger/listen.c:60
msgid   "Connection closed"
msgstr  "Connexion fermé"

#: src/logger/listen.c:69
msgid   "Received incorrect message"
msgstr  "Message incorrect reçu"

#: src/logger/loader.c:75
#, c-format
msgid   "Loader: access to file '%s' failed because %m"
msgstr  "Loader : l’accès au fichier « %s » a échoué parce que %m"

#: src/logger/loader.c:87
#, c-format
msgid   "Loader: failed to load '%s' because %s"
msgstr  "Loader : échec au chargement de « %s » parce que %s"

#: src/logger/main.c:57
msgid   "Storiqoned has hang up"
msgstr  "Le démon a inopinément quitté"

#: src/logger/main.c:81
#, c-format
msgid   "Starting logger process (pid: %d, ppid: %d, sid: %d)"
msgstr  "Démarrage du processus logger (pid : %d, ppid : %d : sid : %d)"

#: src/logger/main.c:82
#, c-format
msgid   "Checksum: %s, last commit: %s"
msgstr  "Empreinte : %s, dernier commit : %s"

#: src/logger/main.c:86
msgid   "No configuration received from daemon, will quit"
msgstr  "Pas de configuration reçu du démon, va maintenant quitter"

#: src/logger/main.c:97
msgid   "Modules loaded"
msgstr  "Modules chargés"

#: src/logger/main.c:110
#, c-format
msgid   "Logger process (pid: %d) will now exit"
msgstr  "Processus logger (pid : %d) va maintenant quitter"
