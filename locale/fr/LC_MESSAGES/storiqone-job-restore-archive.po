#: src/job/restore-archive/check_worker.c:118
#, c-format
msgid   "Skipping verifying '%s' because there are no checksums availables"
msgstr  "Pas de vérification de « %s » parce qu’il n’a pas d’empreintes disponibles"

#: src/job/restore-archive/check_worker.c:129
#, c-format
msgid   "Failed to open '%s' because %m"
msgstr  "Échec à l’ouverture de « %s » parce que %m"

#: src/job/restore-archive/check_worker.c:159
#, c-format
msgid   "data integrity of file '%s' is correct"
msgstr  "l’intégrité des données du fichier « %s » est correcte"

#: src/job/restore-archive/check_worker.c:163
#, c-format
msgid   "data integrity of file '%s' is not correct"
msgstr  "l’intégrité des données du fichier « %s » n’est pas correcte"

#: src/job/restore-archive/data_worker.c:126 src/job/restore-archive/data_worker.c:145
#, c-format
msgid   "Error while seeking to file '%s'"
msgstr  "Erreur en se positionnant sur le fichier « %s »"

#: src/job/restore-archive/data_worker.c:163
#, c-format
msgid   "Creating missing directories '%s' with permission 0777"
msgstr  "Création des répertoires manquants « %s » avec la permission 0777"

#: src/job/restore-archive/data_worker.c:173
#, c-format
msgid   "Starting restoring file '%s'"
msgstr  "Démarrage de la restitution du fichier « %s »"

#: src/job/restore-archive/data_worker.c:180
#, c-format
msgid   "Error while opening file '%s' for writing because %m"
msgstr  "Erreur à l’ouverture du fichier « %s » en écriture parce que %m"

#: src/job/restore-archive/data_worker.c:190
#, c-format
msgid   "Error while seeking into file '%s' because %m"
msgstr  "Erreur en se déplaçant dans le fichier « %s » parce que %m"

#: src/job/restore-archive/data_worker.c:208
#, c-format
msgid   "Error while writing to file '%s' because %m"
msgstr  "Erreur en écrivant dans le fichier « %s » parce que %m"

#: src/job/restore-archive/data_worker.c:217
#, c-format
msgid   "Error while reading from media '%s' because %m"
msgstr  "Erreur en lisant depuis le média « %s » parce que %m"

#: src/job/restore-archive/data_worker.c:225
#, c-format
msgid   "Error while restoring file owner (%s) because %m"
msgstr  "Erreur en restaurant le propriétaire du fichier « %s » parce que %m"

#: src/job/restore-archive/data_worker.c:232
#, c-format
msgid   "Error while restoring file permissions '%s' because %m"
msgstr  "Erreur en restaurant les permissions du fichier « %s » parce que %m"

#: src/job/restore-archive/data_worker.c:243
#, c-format
msgid   "Error while setting file date and time '%s' because %m"
msgstr  "Erreur en restaurant l’horodatage du fichier « %s » parce que %m"

#: src/job/restore-archive/data_worker.c:256
#, c-format
msgid   "Failed to create directory '%s' because %m"
msgstr  "Échec lors de la création du répertoire « %s » parce que %m"

#: src/job/restore-archive/data_worker.c:263
#, c-format
msgid   "Failed to create symbolic link '%s' to '%s' because %m"
msgstr  "Échec lors de la création du lien symbolique « %s » vers « %s » parce que %m"

#: src/job/restore-archive/data_worker.c:268
#, c-format
msgid   "Failed to change permission of '%s' to '%03o' because %m"
msgstr  "Échec lors du changement de permission de « %s » vers « %03o » parce que %m"

#: src/job/restore-archive/data_worker.c:275
#, c-format
msgid   "Failed to create fifo '%s' because %m"
msgstr  "Échec lors de la création du tube nommé « %s » parce que %m"

#: src/job/restore-archive/data_worker.c:282
#, c-format
msgid   "Failed to create character device '%s' because %m"
msgstr  "Échec lors de la création du périphérique characteère « %s » parce que %m"

#: src/job/restore-archive/data_worker.c:290
#, c-format
msgid   "Failed to create block device '%s' because %m"
msgstr  "Échec lors de la création du périphérique bloc « %s » parce que %m"

#: src/job/restore-archive/data_worker.c:298
#, c-format
msgid   "Failed to change owner and group of '%s' because %m"
msgstr  "Échec lors du changement du propriétaire et du groupe de « %s » parce que %m"

#: src/job/restore-archive/data_worker.c:323
msgid   "Failed while setting worker thread name"
msgstr  "Échec lors de la définition du nom du fil d’exécution"

#: src/job/restore-archive/data_worker.c:330
#, c-format
msgid   "Starting worker thread #%u"
msgstr  "Démarrage du fil d’exécution n°%u"

#: src/job/restore-archive/restore-archive.c:281
msgid   "Archive not found"
msgstr  "Archive non trouvée"

#: src/job/restore-archive/restore-archive.c:286
#, c-format
msgid   "No restoration for deleted archive '%s'"
msgstr  "Pas de restitution d’archive supprimée « %s »"

#: src/job/restore-archive/restore-archive.c:294
msgid   "Restore path should be absolute (starts with '/')"
msgstr  "Le chemin de restitution doit être absolu (commence par '/')"

#: src/job/restore-archive/restore-archive.c:306
msgid   "You cannot have \"restore_version\" option with \"restore_min_version\" or \"restore_max_version\""
msgstr  "Vous ne pouvez pas avoir l’option « restore_version » avec « restore_min_version » ou « restore_max_version »"

#: src/job/restore-archive/restore-archive.c:312
msgid   "job option \"restore_min_version\" should be greater or equal to 1"
msgstr  "L’option de tâche « restore_min_version » doit être plus grand ou égale à 1"

#: src/job/restore-archive/restore-archive.c:318
msgid   "job option \"restore_max_version\" should be greater or equal to 1"
msgstr  "L’option de tâche « restore_max_version » doit être plus grand ou égale à 1"

#: src/job/restore-archive/restore-archive.c:324
msgid   "job option \"restore_max_version\" should be greater or equal to \"restore_min_version\""
msgstr  "L’option de tâche « restore_max_version » doit être plus grand ou égale à « restore_min_version »"

#: src/job/restore-archive/restore-archive.c:339
#, c-format
msgid   "Restore version set to %d"
msgstr  "Version de restauration définie à %d"

#: src/job/restore-archive/restore-archive.c:350
#, c-format
msgid   "Wrong restore version \"%d\", should be between 1 and %d"
msgstr  "Mauvaise version de restauration « %d », doit être comprise entre 1 et %d"

#: src/job/restore-archive/restore-archive.c:355
#, c-format
msgid   "Wrong restore version \"%d\", should be greater or equal than -1"
msgstr  "Mauvaise version de restoration « %d », doit être plus grand ou égale à -1"

#: src/job/restore-archive/restore-archive.c:373
#, c-format
msgid   "Will restore from version \"%d\" to \"%d\""
msgstr  "Va restaurer depuis la version « %d » vers « %d »"

#: src/job/restore-archive/restore-archive.c:376
#, c-format
msgid   "Will restore version \"%d\""
msgstr  "Va restaurer la version « %d »"
