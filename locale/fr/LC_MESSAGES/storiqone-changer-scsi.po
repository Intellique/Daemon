#: src/changer/scsi_changer/device.c:157
#, c-format
msgid   "[%s | %s]: unloading media '%s' from drive #%d because media hasn't been used for %lld minute"
msgid_plural    "[%s | %s]: unloading media '%s' from drive #%d because media hasn't been used for %lld minutes"
msgstr[0]       "[ %s | %s ] : déchargement du média « %s » depuis le lecteur n°%d parce que le média n’est plus utilisé "
        "depuis %lld minute"
msgstr[1]       "[ %s | %s ] : déchargement du média « %s » depuis le lecteur n°%d parce que le média n’est plus utilisé "
        "depuis %lld minutes"

#: src/changer/scsi_changer/device.c:165
#, c-format
msgid   "[%s | %s]: unloading media '%s' from drive #%d completed with code = OK"
msgstr  "[ %s | %s ] : déchargement du média « %s » depuis le lecteur n°%d terminé avec le code = OK"

#: src/changer/scsi_changer/device.c:169
#, c-format
msgid   "[%s | %s]: unloading media '%s' from drive #%d completed with code = %d"
msgstr  "[ %s | %s ] : déchargement du média « %s » depuis le lecteur n°%d terminé avec le code = %d"

#: src/changer/scsi_changer/device.c:229
#, c-format
msgid   "Critical, scsi changer %s %s (serial: %s) is not enabled"
msgstr  "Critique, l’échangeur scsi %s %s (numéro de série : %s) n’est pas actif"

#: src/changer/scsi_changer/device.c:244 src/changer/scsi_changer/device.c:263
#, c-format
msgid   "Failed while reading link of file '%s' because %m"
msgstr  "Échec en lisant le lien symbolique du fichier « %s » parce que %m"

#: src/changer/scsi_changer/device.c:296
#, c-format
msgid   "Found scsi changer %s %s, serial: %s"
msgstr  "Trouvé un échangeur scsi %s %s, numéro de série : %s"

#: src/changer/scsi_changer/device.c:305
#, c-format
msgid   "Critical, scsi changer %s %s (serial: %s) was not found"
msgstr  "Critique, l’échangeur scsi %s %s (numéro de série : %s) n’a pas été trouvé"

#: src/changer/scsi_changer/device.c:364
#, c-format
msgid   "Warning, scsi changer %s %s (serial: %s) is offline"
