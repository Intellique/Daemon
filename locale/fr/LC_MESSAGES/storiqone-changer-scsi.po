#: src/changer/scsi_changer/device.c:383
#, c-format
msgid   "[%s | %s]: starting check media with drive #%u"
msgstr  "[ %s | %s ] : démarrage de la vérification de média avec le lecteur n°%u"

#: src/changer/scsi_changer/device.c:394 src/changer/scsi_changer/device.c:470
#, c-format
msgid   "[%s | %s]: failed to get status from drive #%u %s %s"
msgstr  "[ %s | %s ] : échec lors de la réinitialisation du lecteur n°%u %s %s"

#: src/changer/scsi_changer/device.c:402 src/changer/scsi_changer/device.c:459 src/changer/scsi_changer/device.c:478
#: src/changer/scsi_changer/device.c:546
#, c-format
msgid   "[%s | %s]: checking media with drive #%u completed"
msgstr  "[ %s | %s ] : vérification du média avec le lecteur n°%u terminée"

#: src/changer/scsi_changer/device.c:422
msgid   "<UNLABELED>"
msgstr  "<SANS ÉTIQUETTE>"

#: src/changer/scsi_changer/device.c:431
#, c-format
msgid   "[%s | %s]: loading media '%s' from slot #%u to drive #%u"
msgstr  "[ %s | %s ] : chargement du média « %s » depuis l’emplacement n°%u vers le lecteur n°%u"

#: src/changer/scsi_changer/device.c:440
#, c-format
msgid   "[%s | %s]: loading media '%s' from slot #%u to drive #%u completed with code = OK"
