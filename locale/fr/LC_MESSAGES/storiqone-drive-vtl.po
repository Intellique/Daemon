#: src/drive/vtl_drive/device.c:200
#, c-format
msgid   "[%s %s %d]: Failed to erase file '%s' because %m"
msgstr  "[ %s %s %d ] : Échec lors de l’effacement du fichier « %s » parce que %m"

#: src/drive/vtl_drive/device.c:220 src/drive/vtl_drive/device.c:316 src/drive/vtl_drive/device.c:465
#, c-format
msgid   "[%s %s %d]: Failed to list files from media '%s'"
msgstr  "[ %s %s %d ] : Échec de l’énumération des fichiers du média « %s »"

#: src/drive/vtl_drive/device.c:222 src/drive/vtl_drive/device.c:318 src/drive/vtl_drive/device.c:467
msgid   "NULL"
msgstr  "VIDE"

#: src/drive/vtl_drive/device.c:251
#, c-format
msgid   "[%s %s %d]: Command 'shred' ended with code '%d' while removing file '%s'"
msgstr  "[ %s %s %d ] : Commande « shred » a quitté avec le code « %d » lors de la suppression du fichier « %s »"

#: src/drive/vtl_drive/device.c:257
#, c-format
msgid   "[%s %s %d]: Failed to start 'shred' command to remove file '%s'"
msgstr  "[ %s %s %d ] : Échec du lancement de la commande « shred » pour supprimer le fichier « %s »"

#: src/drive/vtl_drive/device.c:298
#, c-format
msgid   "[%s %s %d]: VTL does only support Storiq One format"
msgstr  "[ %s %s %d ] : VTL ne supporte que le format « Storiq One »"

#: src/drive/vtl_drive/device.c:342
#, c-format
msgid   "[%s %s #%u]: Using default block size (%s) for media '%s'"
msgstr  "[ %s %s %u ] : Utilisation de la taille de bloc par défaut (%s) pour le média « %s »"

#: src/drive/vtl_drive/device.c:359
#, c-format
msgid   "[%s %s %d]: Failed to write media header '%s'"
msgstr  "[ %s %s %d ] : Échec lors de l’écriture de l’en-tête du média « %s »"

#: src/drive/vtl_drive/device.c:446
#, c-format
msgid   "[%s %s %d]: Failed to open file from media '%s' at position '%d' because %m"
msgstr  "[ %s %s %d ] : Échec lors de l’ouverture du fichier du média « %s » à la position « %d » parce que %m"

#: src/drive/vtl_drive/device.c:575
#, c-format
msgid   "[%s %s %d]: Failed to find media with UUID '%s' from database"
