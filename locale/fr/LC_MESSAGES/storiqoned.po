#: src/storiqoned/main.c:125
msgid   "Parsing options: OK"
msgstr  "Analyse des options : OK"

#: src/storiqoned/main.c:152
msgid   "Administration not configured"
msgstr  "Administration non configurée"

#: src/storiqoned/main.c:198
msgid   "Daemon will shut down"
msgstr  "Le démon va s’arrêter"

#: src/storiqoned/main.c:210
#, c-format
msgid   "    --config=FILE,   -c : Uses this configuration FILE instead of “%s”\n"
msgstr  "    --config=FICHIER,   -c : utilise ce FICHIER de configuration à la place de « %s »\n"

#: src/storiqoned/main.c:211
#, c-format
msgid   "    --help,          -h : Shows this help and exits\n"
msgstr  "    --help,             -h : Affiche cette aide et quitte\n"

#: src/storiqoned/main.c:212
#, c-format
msgid   "    --pid-file=FILE, -p : Writes the pid of daemon into FILE instead of “%s”\n"
msgstr  "    --pid-file=FICHIER, -p : Écrit le pid du démon dans FICHIER au lieu de « %s »\n"

#: src/storiqoned/main.c:213
#, c-format
msgid   "    --version,       -V : prints StoriqOne version number and exits\n"
msgstr  "    --version,          -V : Affiche la version de StoriqOne puis quitte\n"
