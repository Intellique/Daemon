#: src/job/format-media/format-media.c:94
#, c-format
msgid   "Failed to load media '%s'"
msgstr  "Échec lors du chargement du média « %s »"

#: src/job/format-media/format-media.c:110
#, c-format
msgid   "Try to format a write protected media '%s'"
msgstr  "Tentative de formater un média « %s » qui est protégé en écriture"

#: src/job/format-media/format-media.c:117
#, c-format
msgid   "Formatting media '%s' in progress (block size used: %zd)"
msgstr  "Formatage du média « %s » en cours (taille de bloc utilisée : %zd)"

#: src/job/format-media/format-media.c:121
#, c-format
msgid   "Formatting media '%s' in progress"
msgstr  "Formatage du média « %s » en cours"

#: src/job/format-media/format-media.c:128
msgid   "Failed to format media"
msgstr  "Échec du formatge du média"

#: src/job/format-media/format-media.c:137
#, c-format
msgid   "Checking media header '%s' in progress"
msgstr  "Vérification de l’entête du média « %s » en cours"

#: src/job/format-media/format-media.c:142
#, c-format
msgid   "Checking media header '%s': success"
msgstr  "La vérification de l’entête du média « %s » a réussi"

#: src/job/format-media/format-media.c:150
#, c-format
msgid   "Checking media header '%s': failed"
msgstr  "Vérification de l’entête du média « %s » : échec"

#: src/job/format-media/format-media.c:162
msgid   "No pool related to this job"
msgstr  "Pas de pool lié à cette tâche"

#: src/job/format-media/format-media.c:169
msgid   "No media linked to the current job"
msgstr  "Pas de média lié à la tâche actuelle"

#: src/job/format-media/format-media.c:174
#, c-format
msgid   "Try to format a cleaning media '%s'"
msgstr  "Tentative de formater un média de nettoyage « %s »"

#: src/job/format-media/format-media.c:180
#, c-format
msgid   "Try to format a worm media '%s' with data"
msgstr  "Tentative de formater un média non réinscriptible « %s » qui contient des données"

#: src/job/format-media/format-media.c:186
#, c-format
msgid   "Try to format a media '%s' which is a member of pool '%s'"
msgstr  "Tentative de formater un média « %s » qui est membre du pool « %s »"

#: src/job/format-media/format-media.c:192
#, c-format
msgid   "Try to format a media '%s' with error status"
msgstr  "Tentative de formater un média « %s » qui est en erreur"

#: src/job/format-media/format-media.c:197
#, c-format
msgid   "Try to format a media '%s' to a pool '%s' which is deleted"
msgstr  "Tentative de formater un média « %s » dans un pool supprimé « %s »"

#: src/job/format-media/format-media.c:204
#, c-format
msgid   "Try to format a media '%s' (media format: %s) whose type does not match the format of pool '%s' (fomat: %s)"
msgstr  "Tentative de formater un média « %s » (format du média : %s) dont le format ne correspond pas au format du pool "
        "« %s » (format du pool : %s)"

#: src/job/format-media/format-media.c:211
#, c-format
msgid   "Failed to find suitable drive to format media '%s'"
msgstr  "Échec lors de la recherche d’un lecteur pour formater le média « %s »"

#: src/job/format-media/format-media.c:229
#, c-format
msgid   "Will use default block size '%s' defined by media format '%s'"
msgstr  "Utilisera la taille de bloque par défaut « %s » définie le format du média « %s »"

#: src/job/format-media/format-media.c:238
#, c-format
msgid   "Will use default block size '%s' defined by media format '%s' because type of media '%s' is worm media"
msgstr  "Utilisera la taille de bloque « %s » par défaut définie par le format du média « %s » parce que le type du média "
        "« %s » est WORM"

#: src/job/format-media/format-media.c:244
#, c-format
msgid   "Will find the best block size to use with media '%s'"
msgstr  "Cherchera la meilleur taille de bloque à utiliser avec le média « %s »"

#: src/job/format-media/format-media.c:254
#, c-format
msgid   "parameter 'block size' should be a positive integer (not %zd)"
msgstr  "Le paramètre « block size » doit être un entier positif (pas %zd)"

#: src/job/format-media/format-media.c:328
msgid   "Invalid uuid provide by script, ignoring it"
msgstr  "uuid invalide fourni par le script"

#: src/job/format-media/format-media.c:331
#, c-format
msgid   "Script request to format media with this uuid (%s) instead of (%s)"
msgstr  "Le script demande de formater le média avec cet uuid (%s) au lieur de (%s)"

#: src/job/format-media/format-media.c:342
#, c-format
msgid   "Script request to format media with this name (%s) instead of (%s)"
msgstr  "Le scripte demande de formater le média avec ce nom (%s) au lieur de (%s)"
