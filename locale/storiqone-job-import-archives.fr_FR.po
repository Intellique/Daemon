#: src/job/import-archives/import-archives.c:117
msgid   "Find first archive"
msgstr  "Recherche de la première archive"

#: src/job/import-archives/import-archives.c:159
msgid   "Critic error, no media linked to the current job"
msgstr  "Erreur critique, pas de média lié à la tâche actuelle"

#: src/job/import-archives/import-archives.c:164
#, c-format
msgid   "Try to import a cleaning media '%s'"
msgstr  "Tentative d’importation d’une média de netoyage « %s »"

#: src/job/import-archives/import-archives.c:170
#, c-format
