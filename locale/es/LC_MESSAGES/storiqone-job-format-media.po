#: src/job/format-media/format-media.c:94
#, c-format
msgid   "Failed to load media '%s'"
msgstr  "Fallo al cargar el media \"%s\""

#: src/job/format-media/format-media.c:110
#, c-format
msgid   "Trying to format a write protected media '%s'"
msgstr  "Tratando de formatear un media \"%s\" que está protegido"

#: src/job/format-media/format-media.c:117
#, c-format
msgid   "Formatting media '%s' in progress (block size used: %zd)"
msgstr  "formateando el media \"%s\" en progreso (tamaño del bloque utilizado: %zd)"

#: src/job/format-media/format-media.c:121
#, c-format
msgid   "Formatting media '%s' in progress"
msgstr  "Formateando el media \"%s\" en progreso"

#: src/job/format-media/format-media.c:128
msgid   "Failed to format media"
msgstr  "Fallo al formatear lo medios"

#: src/job/format-media/format-media.c:137
#, c-format
msgid   "Checking media header '%s' in progress"
msgstr  "Verificación del encabezamiento del media \"%s\" en progreso"

#: src/job/format-media/format-media.c:142
#, c-format
msgid   "Checking media header '%s' was successful"
msgstr  "Verificación del encabezamiento \"%s\" a sido exitoso"

#: src/job/format-media/format-media.c:150
#, c-format
msgid   "Checking media header '%s': failed"
msgstr  "Verificacón del encabezamiento del media \"%s\": Fallo"

#: src/job/format-media/format-media.c:220
msgid   "Invalid UUID provided by script, ignoring it"
msgstr  "UUID inválido proporcionado por el script, ignorado"

#: src/job/format-media/format-media.c:223
#, c-format
msgid   "Script request to format media with this UUID (%s) instead of (%s)"
msgstr  "El script pide formatear el media con UUID (%s) en lugar de (%s)"

#: src/job/format-media/format-media.c:234
#, c-format
msgid   "Script request to format media with this name (%s) instead of (%s)"
msgstr  "El script pide formatear el media con el nombre (%s) en lugar de (%s)"

#: src/job/format-media/format-media.c:253
msgid   "No pool related to this job"
msgstr  "No hay pool liado a ésta tarea"

#: src/job/format-media/format-media.c:260
msgid   "No media linked to the current job"
msgstr  "No hay media liado a la tarea actual"

#: src/job/format-media/format-media.c:265
#, c-format
msgid   "Trying to format a cleaning media '%s'"
msgstr  "Trantando de formatear un media de limpieza \"%s\""

#: src/job/format-media/format-media.c:271
#, c-format
msgid   "Trying to format a worm media '%s' containing data"
msgstr  "Tratando de formatear un media no regradable \"%s\" que contiene datos "

#: src/job/format-media/format-media.c:277
#, c-format
msgid   "Trying to format a media '%s' which is member of pool '%s'"
msgstr  "tratando de formatear  \"%s\" que es miembro de un pool \"%s\""

#: src/job/format-media/format-media.c:283
#, c-format
msgid   "Trying to format a media '%s' with error status"
msgstr  "Tratando de formatear un media \"%s\" "

#: src/job/format-media/format-media.c:288
#, c-format
msgid   "Trying to format a media '%s' to a deleted pool '%s'"
msgstr  "tratando de formatear un media \"%s\" que está en error \"%s\""

#: src/job/format-media/format-media.c:295
#, c-format
msgid   "Trying to format a media '%s' (media format: %s) not matching pool '%s' (pool format: %s)"
msgstr  "Tratando de formatear un media \"%s\" (formato del media: %s) donde el nombre no corresponde al formato del pool \"%s"
        "\" (format du pool: %s)"

#: src/job/format-media/format-media.c:302
#, c-format
msgid   "Failed to find a suitable drive to format media '%s'"
msgstr  "ho hay lector disponible para formatear el media \"%s\""

#: src/job/format-media/format-media.c:320
#, c-format
msgid   "Will use default block size '%s' defined by media format '%s'"
msgstr  "Utilise el tamaño del boque predeterminado \"%s\" defina el formato del media \"%s\""

#: src/job/format-media/format-media.c:329
#, c-format
msgid   "Will use default block size '%s' defined by media format '%s' because type of media '%s' is WORM"
msgstr  "Utilizará el tamaño del bloque \"%s\" predeterminado por el formato de media \"%s\" porque el tipo de media \"%s\" es "
        "WORM"

#: src/job/format-media/format-media.c:335
#, c-format
msgid   "Will look for the best block size to use with media '%s'"
msgstr  "Buscará el mejor tamaño del bloque para utilizarlo con el media \"%s\""

#: src/job/format-media/format-media.c:347
#, c-format
msgid   "parameter 'block size' should be a positive integer (not %zd)"
msgstr  "El parámetro \"block size\" debe ser un número entero positivo (no %zd)"
