#, c-format
msgid   "No write support for format '%s'"
msgstr  "No hay soporte para el formato en escritura \"%s\""

#: src/job/format-media/format-media.c:346
#, c-format
msgid   "Trying to format a media '%s' (media format: %s) not matching pool '%s' (pool format: %s)"
msgstr  "Tratando de formatear un media \"%s\" (formato del media: %s) donde el nombre no corresponde al formato del pool \"%s"
        "\" (formato del pool: %s)"

#: src/job/format-media/format-media.c:353
#, c-format
msgid   "Failed to find a suitable drive to format media '%s'"
msgstr  "ho hay lector disponible para formatear el media \"%s\""

#: src/job/format-media/format-media.c:374
#, c-format
msgid   "Will use default block size '%s' defined by media format '%s'"
msgstr  "Utilise el tamaño del boque predeterminado \"%s\" defina el formato del media \"%s\""

#: src/job/format-media/format-media.c:383
#, c-format
msgid   "Will use default block size '%s' defined by media format '%s' because type of media '%s' is WORM"
msgstr  "Utilizará el tamaño del bloque \"%s\" predeterminado por el formato de media \"%s\" porque el tipo de media \"%s\" es "
        "WORM"

#: src/job/format-media/format-media.c:389
#, c-format
msgid   "Will look for the best block size to use with media '%s'"
msgstr  "Buscará el mejor tamaño del bloque para utilizarlo con el media \"%s\""

#: src/job/format-media/format-media.c:397
#, c-format
msgid   "Invalid block size '%s'"
msgstr  "Tamaño del bloque no valido \"%s\""

#: src/job/format-media/format-media.c:409
#, c-format
msgid   "parameter 'block size' should be a positive integer (not %zd)"
msgstr  "El parámetro \"block size\" debe ser un número entero positivo (no %zd)"

