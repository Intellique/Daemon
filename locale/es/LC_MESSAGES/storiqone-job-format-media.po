#, c-format
msgid   "Trying to format a worm media '%s' containing data"
msgstr  "Tratando de formatear un media no regradable \"%s\" que contiene datos "

#: src/job/format-media/format-media.c:309
#, c-format
msgid   "Trying to format a media '%s' which is member of pool '%s'"
msgstr  "tratando de formatear  \"%s\" que es miembro de un pool \"%s\""

#: src/job/format-media/format-media.c:315
#, c-format
msgid   "Trying to format a media '%s' with error status"
msgstr  "Tratando de formatear un media \"%s\" "

#: src/job/format-media/format-media.c:321
msgid   "No pool related to this job"
msgstr  "No hay pool liado a ésta tarea"

#: src/job/format-media/format-media.c:326
#, c-format
msgid   "Trying to format a media '%s' to a deleted pool '%s'"
msgstr  "tratando de formatear un media \"%s\" que está en error \"%s\""

#: src/job/format-media/format-media.c:333
#, c-format
msgid   "Trying to format a LTFS media '%s' which does not support partition (LTFS requirement)"
msgstr  "Intento al formatear un media \"%s\" en LTFS que no soporte las particiones (prerequeridas LTFS "

