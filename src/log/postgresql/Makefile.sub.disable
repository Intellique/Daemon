STONE_LOG_POSTGRESQL_SRC_DIR		:= src/log/postgresql

STONE_LOG_POSTGRESQL_BIN			:= lib/liblog-postgresql.so
STONE_LOG_POSTGRESQL_CFLAG			:= -pthread -fPIC -fvisibility=hidden
STONE_LOG_POSTGRESQL_LD				:= -pthread -shared -lstone -Llib -lpq

STONE_LOG_POSTGRESQL_DEPEND_LIB		:= lib/libstone.so

STONE_LOG_POSTGRESQL_CHCKSUM_FILE	:= liblog-postgresql.chcksum

BIN_SYMS							+= STONE_LOG_POSTGRESQL
