STORIQ_ARCHIVER_CHECKSUM_SHA512_SRC_DIR		:= src/module/checksum/sha512
STORIQ_ARCHIVER_CHECKSUM_SHA512_BUILD_DIR	:= ${BUILD_DIR}/module/checksum/sha512
STORIQ_ARCHIVER_CHECKSUM_SHA512_DEPEND_DIR	:= ${DEPEND_DIR}/module/checksum/sha512

STORIQ_ARCHIVER_CHECKSUM_SHA512_SRC_FILES	:= $(sort $(shell test -d ${STORIQ_ARCHIVER_CHECKSUM_SHA512_SRC_DIR} && find ${STORIQ_ARCHIVER_CHECKSUM_SHA512_SRC_DIR} -name '*.c'))
STORIQ_ARCHIVER_CHECKSUM_SHA512_HEAD_FILES	:= $(sort $(shell test -d ${STORIQ_ARCHIVER_CHECKSUM_SHA512_SRC_DIR} && find ${STORIQ_ARCHIVER_CHECKSUM_SHA512_SRC_DIR} -name '*.h'))
STORIQ_ARCHIVER_CHECKSUM_SHA512_OBJ_FILES	:= $(sort $(patsubst src/%.c,${BUILD_DIR}/%.o,${STORIQ_ARCHIVER_CHECKSUM_SHA512_SRC_FILES}))
STORIQ_ARCHIVER_CHECKSUM_SHA512_DEP_FILES	:= $(sort $(shell test -d ${STORIQ_ARCHIVER_CHECKSUM_SHA512_DEPEND_DIR} && find ${STORIQ_ARCHIVER_CHECKSUM_SHA512_DEPEND_DIR} -name '*.d'))

STORIQ_ARCHIVER_CHECKSUM_SHA512_BIN			:= lib/checksum/libsha512.so
STORIQ_ARCHIVER_CHECKSUM_SHA512_CFLAG		:= -fPIC -fvisibility=hidden
STORIQ_ARCHIVER_CHECKSUM_SHA512_LD			:= -shared -lssl

STORIQ_ARCHIVER_CHECKSUM_SHA512_LIB			:=

BIN_SYMS									+= STORIQ_ARCHIVER_CHECKSUM_SHA512
