STORIQ_ARCHIVER_CHECKSUM_CRC32_DEPEND_DIR	:= ${DEPEND_DIR}/module/checksum/crc32

STORIQ_ARCHIVER_CHECKSUM_CRC32_SRC_FILES		:= $(sort $(shell test -d ${STORIQ_ARCHIVER_CHECKSUM_CRC32_SRC_DIR} && find ${STORIQ_ARCHIVER_CHECKSUM_CRC32_SRC_DIR} -name '*.c'))
STORIQ_ARCHIVER_CHECKSUM_CRC32_HEAD_FILES		:= $(sort $(shell test -d ${STORIQ_ARCHIVER_CHECKSUM_CRC32_SRC_DIR} && find ${STORIQ_ARCHIVER_CHECKSUM_CRC32_SRC_DIR} -name '*.h'))
