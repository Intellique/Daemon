STORIQ_ARCHIVER_CHECKSUM_MD5_SRC_DIR		:= src/module/checksum/md5
STORIQ_ARCHIVER_CHECKSUM_MD5_BUILD_DIR		:= ${BUILD_DIR}/module/checksum/md5
STORIQ_ARCHIVER_CHECKSUM_MD5_DEPEND_DIR		:= ${DEPEND_DIR}/module/checksum/md5

STORIQ_ARCHIVER_CHECKSUM_MD5_SRC_FILES		:= $(sort $(shell test -d ${STORIQ_ARCHIVER_CHECKSUM_MD5_SRC_DIR} && find ${STORIQ_ARCHIVER_CHECKSUM_MD5_SRC_DIR} -name '*.c'))
STORIQ_ARCHIVER_CHECKSUM_MD5_HEAD_FILES		:= $(sort $(shell test -d ${STORIQ_ARCHIVER_CHECKSUM_MD5_SRC_DIR} && find ${STORIQ_ARCHIVER_CHECKSUM_MD5_SRC_DIR} -name '*.h'))
STORIQ_ARCHIVER_CHECKSUM_MD5_OBJ_FILES		:= $(sort $(patsubst src/%.c,${BUILD_DIR}/%.o,${STORIQ_ARCHIVER_CHECKSUM_MD5_SRC_FILES}))
STORIQ_ARCHIVER_CHECKSUM_MD5_DEP_FILES		:= $(sort $(shell test -d ${STORIQ_ARCHIVER_CHECKSUM_MD5_DEPEND_DIR} && find ${STORIQ_ARCHIVER_CHECKSUM_MD5_DEPEND_DIR} -name '*.d'))

STORIQ_ARCHIVER_CHECKSUM_MD5_BIN			:= lib/checksum/libmd5.so
STORIQ_ARCHIVER_CHECKSUM_MD5_CFLAG			:= -fPIC -fvisibility=hidden
STORIQ_ARCHIVER_CHECKSUM_MD5_LD				:= -shared -lssl

STORIQ_ARCHIVER_CHECKSUM_MD5_LIB			:=

BIN_SYMS									+= STORIQ_ARCHIVER_CHECKSUM_MD5
