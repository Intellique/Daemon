STORIQ_ARCHIVER_LOG_FILE_SRC_DIR	:= src/module/log/file
STORIQ_ARCHIVER_LOG_FILE_BUILD_DIR	:= ${BUILD_DIR}/module/log/file
STORIQ_ARCHIVER_LOG_FILE_DEPEND_DIR	:= ${DEPEND_DIR}/module/log/file

STORIQ_ARCHIVER_LOG_FILE_SRC_FILES	:= $(sort $(shell test -d ${STORIQ_ARCHIVER_LOG_FILE_SRC_DIR} && find ${STORIQ_ARCHIVER_LOG_FILE_SRC_DIR} -name '*.c'))
STORIQ_ARCHIVER_LOG_FILE_HEAD_FILES	:= $(sort $(shell test -d ${STORIQ_ARCHIVER_LOG_FILE_SRC_DIR} && find ${STORIQ_ARCHIVER_LOG_FILE_SRC_DIR} -name '*.h'))
STORIQ_ARCHIVER_LOG_FILE_OBJ_FILES	:= $(sort $(patsubst src/%.c,${BUILD_DIR}/%.o,${STORIQ_ARCHIVER_LOG_FILE_SRC_FILES}))
STORIQ_ARCHIVER_LOG_FILE_DEP_FILES	:= $(sort $(shell test -d ${STORIQ_ARCHIVER_LOG_FILE_DEPEND_DIR} && find ${STORIQ_ARCHIVER_LOG_FILE_DEPEND_DIR} -name '*.d'))

STORIQ_ARCHIVER_LOG_FILE_BIN		:= lib/log/libfile.so
STORIQ_ARCHIVER_LOG_FILE_CFLAG		:= -fPIC
STORIQ_ARCHIVER_LOG_FILE_LD			:= -shared

STORIQ_ARCHIVER_LOG_FILE_LIB		:=

BIN_SYMS							+= STORIQ_ARCHIVER_LOG_FILE
