STORIQ_ARCHIVER_DB_POSTGRESQL_SRC_DIR		:= src/module/db/postgresql
STORIQ_ARCHIVER_DB_POSTGRESQL_BUILD_DIR		:= ${BUILD_DIR}/module/db/postgresql
STORIQ_ARCHIVER_DB_POSTGRESQL_DEPEND_DIR	:= ${DEPEND_DIR}/module/db/postgresql

STORIQ_ARCHIVER_DB_POSTGRESQL_SRC_FILES		:= $(sort $(shell test -d ${STORIQ_ARCHIVER_DB_POSTGRESQL_SRC_DIR} && find ${STORIQ_ARCHIVER_DB_POSTGRESQL_SRC_DIR} -name '*.c'))
STORIQ_ARCHIVER_DB_POSTGRESQL_HEAD_FILES	:= $(sort $(shell test -d ${STORIQ_ARCHIVER_DB_POSTGRESQL_SRC_DIR} && find ${STORIQ_ARCHIVER_DB_POSTGRESQL_SRC_DIR} -name '*.h'))
STORIQ_ARCHIVER_DB_POSTGRESQL_OBJ_FILES		:= $(sort $(patsubst src/%.c,${BUILD_DIR}/%.o,${STORIQ_ARCHIVER_DB_POSTGRESQL_SRC_FILES}))
STORIQ_ARCHIVER_DB_POSTGRESQL_DEP_FILES		:= $(sort $(shell test -d ${STORIQ_ARCHIVER_DB_POSTGRESQL_DEPEND_DIR} && find ${STORIQ_ARCHIVER_DB_POSTGRESQL_DEPEND_DIR} -name '*.d'))

STORIQ_ARCHIVER_DB_POSTGRESQL_BIN			:= lib/db/libpostgresql.so
STORIQ_ARCHIVER_DB_POSTGRESQL_CFLAG			:= -fPIC -fvisibility=hidden
STORIQ_ARCHIVER_DB_POSTGRESQL_LD			:= -shared -lpq

STORIQ_ARCHIVER_DB_POSTGRESQL_LIB			:=

BIN_SYMS									+= STORIQ_ARCHIVER_DB_POSTGRESQL
