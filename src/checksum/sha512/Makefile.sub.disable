STONE_CHECKSUM_SHA512_SRC_DIR		:= src/checksum/sha512

STONE_CHECKSUM_SHA512_BIN			:= lib/libchecksum-sha512.so
STONE_CHECKSUM_SHA512_CFLAG			:= -pthread -fPIC -fvisibility=hidden
STONE_CHECKSUM_SHA512_LD			:= -pthread -shared -lstone -Llib -lcrypto

STONE_CHECKSUM_SHA512_DEPEND_LIB	:= lib/libstone.so

STONE_CHECKSUM_SHA512_CHCKSUM_FILE	:= libchecksum-sha512.chcksum

BIN_SYMS							+= STONE_CHECKSUM_SHA512
