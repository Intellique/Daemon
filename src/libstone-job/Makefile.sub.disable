LIBSTONEJOB_SRC_DIR			:= src/libstone-job

LIBSTONEJOB_CFLAG			:= -fPIC -pthread
LIBSTONEJOB_LIB				:= lib/libstone-job.so
LIBSTONEJOB_LD				:= -shared -pthread -ldl -lstone -Llib -Wl,--version-script,src/libstone-job/libstone-job.map
LIBSTONEJOB_SONAME			:= libstone-job.so.1
LIBSTONEJOB_LIB_VERSION		:= 1.2

LIBSTONEJOB_DEPEND_LIB		:= src/libstone-job/libstone-job.map lib/libstone.so

LIBSTONEJOB_CHCKSUM_FILE	:= libstone-job.chcksum

BIN_SYMS					+= LIBSTONEJOB
