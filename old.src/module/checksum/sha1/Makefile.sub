STORIQ_ARCHIVER_CHECKSUM_SHA1_SRC_DIR		:= src/module/checksum/sha1
STORIQ_ARCHIVER_CHECKSUM_SHA1_BUILD_DIR		:= ${BUILD_DIR}/module/checksum/sha1
STORIQ_ARCHIVER_CHECKSUM_SHA1_DEPEND_DIR	:= ${DEPEND_DIR}/module/checksum/sha1

STORIQ_ARCHIVER_CHECKSUM_SHA1_SRC_FILES		:= $(sort $(shell test -d ${STORIQ_ARCHIVER_CHECKSUM_SHA1_SRC_DIR} && find ${STORIQ_ARCHIVER_CHECKSUM_SHA1_SRC_DIR} -name '*.c'))
STORIQ_ARCHIVER_CHECKSUM_SHA1_HEAD_FILES	:= $(sort $(shell test -d ${STORIQ_ARCHIVER_CHECKSUM_SHA1_SRC_DIR} && find ${STORIQ_ARCHIVER_CHECKSUM_SHA1_SRC_DIR} -name '*.h'))
STORIQ_ARCHIVER_CHECKSUM_SHA1_OBJ_FILES		:= $(sort $(patsubst src/%.c,${BUILD_DIR}/%.o,${STORIQ_ARCHIVER_CHECKSUM_SHA1_SRC_FILES}))
STORIQ_ARCHIVER_CHECKSUM_SHA1_DEP_FILES		:= $(sort $(shell test -d ${STORIQ_ARCHIVER_CHECKSUM_SHA1_DEPEND_DIR} && find ${STORIQ_ARCHIVER_CHECKSUM_SHA1_DEPEND_DIR} -name '*.d'))

STORIQ_ARCHIVER_CHECKSUM_SHA1_BIN			:= lib/checksum/libsha1.so
STORIQ_ARCHIVER_CHECKSUM_SHA1_CFLAG			:= -fPIC -fvisibility=hidden
STORIQ_ARCHIVER_CHECKSUM_SHA1_LD			:= -shared -lssl

STORIQ_ARCHIVER_CHECKSUM_SHA1_LIB			:=

BIN_SYMS									+= STORIQ_ARCHIVER_CHECKSUM_SHA1
